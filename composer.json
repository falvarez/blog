{
    "name": "falvarez/blog",
    "description": "Blog source code based on Sculpin, inspired by https://github.com/sculpin/sculpin-blog-skeleton, built on Clean Blog Theme http://startbootstrap.com/template-overviews/clean-blog/",
    "minimum-stability": "stable",
    "license": "CC Attribution-NonCommercial-NoDerivatives 4.0 International",
    "authors": [
        {
            "name": "Federico J. √Ålvarez Valero",
            "email": "fede.alvarez@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Falvarez\\Blog\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/falvarez/sculpin.git"
        }
    ],
    "require-dev": {
        "sculpin/sculpin": "dev-master",
        "jasny/twig-extensions": "^1.3",
        "symfony/http-foundation": "^5.0.7",
        "twig/intl-extra": "^3.13.0"
    },
    "config": {
        "allow-plugins": {
            "sculpin/sculpin-theme-composer-plugin": true
        },
        "preferred-install": {
            "sculpin/sculpin": "source",
            "*": "dist"
        }
    },
  "require": {
    "ext-intl": "*"
  }
}
